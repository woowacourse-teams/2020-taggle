{
  "name": "Taggle",
  "description": "태그단위로 당신의 북마크를 효율적으로 관리하세요!",
  "version": null,
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "taggle-extension",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["tabs"]
}
