{
  "name": "Taggle",
  "description": "효율적인 북마크 관리를 위한 Taggle 확장프로그램",
  "version": "1.0.0",
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "96": "icons/icon_96.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "taggle-extension",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs",
    "cookies",
    "https://taggle.kr/*"
  ]
}
